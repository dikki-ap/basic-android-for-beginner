<resources>
    <string name="app_name">Basic Android</string>

    <!--  U can use "%index$type to add params in String Resources  -->
    <!--  "d" for decimal, "s" for string  -->
    <string name="other_text">Other Text %1$s</string>

    <!--  U can make an Array to in String Resources  -->
    <string-array name="names">
        <item>Dikki</item>
        <item>Agresia</item>
        <item>Janan</item>
    </string-array>
    <string name="title_activity_intent_object">IntentActivityObject</string>
    <!-- Strings used for fragments for navigation -->
    <string name="first_fragment_label">First Fragment</string>
    <string name="second_fragment_label">Second Fragment</string>
    <string name="next">Next</string>
    <string name="previous">Previous</string>

    <string name="hello_first_fragment">Hello first fragment</string>
    <string name="hello_second_fragment">Hello second fragment. Arg: %1$s</string>

    <!--  U can add another Data Type into this String Resources, u can see the documentation  -->
</resources>